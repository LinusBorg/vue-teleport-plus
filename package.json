{
  "name": "vue-teleport-plus",
  "version": "0.0.0",
  "description": "A monorepo template for developing vue libraries",
  "author": "Thorsten LÃ¼nborg <thorsten@linusb.org>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "lerna run start --stream --parallel --scope '@linusborg/*'",
    "test": "lerna run test --stream --scope '@linusborg/*'  --",
    "lint": "lerna run lint -- --fix",
    "build": "lerna run build --scope '@linusborg/*'",
    "play": "cd packages/playground && yarn start",
    "cypress": "cypress run --config-file cypress/cypress.json",
    "cypress:open": "cypress open --config-file cypress/cypress.json"
  },
  "devDependencies": {
    "@linusborg/eslint-config": "^0.1.0",
    "@testing-library/cypress": "^7.0.3",
    "@types/jest": "^26.0.20",
    "@vitejs/plugin-vue": "^1.0.4",
    "@vue/compiler-sfc": "^3.0.4",
    "@vue/test-utils": "^2.0.0-beta.14",
    "autoprefixer": "^10.1.0",
    "cypress": "8.0.0",
    "eslint": "^7.17.0",
    "jest": "^27.0.0",
    "lerna": "^3.15.0",
    "prettier": "^2.2.1",
    "rollup-plugin-dts": "^2.0.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.1.0",
    "typescript-vue-plugin": "^0.26.0-0",
    "vite": "^2.0.0",
    "vue": "^3.0.5",
    "vue-jest": "^5.0.0-alpha.7"
  }
}
